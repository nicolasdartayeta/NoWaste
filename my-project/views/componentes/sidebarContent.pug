mixin item(item)
    li(id=item.id)
        console.log(item.title)
        button(hx-get=item.requestURL hx-target=item.target hx-push-url='true' class="w-full flex justify-start rounded-lg px-4 py-2 text-sm font-medium bg-gray-100/50 hover:bg-gray-100 hover:text-gray-700")=item.title 


p(class='text-lg mt-5')=sidebar.title 

ul(class="mt-3 space-y-1 flex flex-col w-full")
    each item in sidebar.items
        +item(item)